UPDATE mrp_bom_line 
	SET  bom_id = 54
	WHERE bom_id = 4	

SELECT * FROM product_product where id = 182;
select * from product_template where id = 243;
update product_template set uom_id = 54 where id = 243;

-- lista de materiales
UPDATE mrp_bom_line 
	SET  bom_id = 127
	WHERE bom_id IN(81,84,85)
	
update product_template 
	set uom_id = 127
	WHERE uom_id IN (81,84,85)

update product_template 
	set uom_po_id = 127
	WHERE uom_po_id IN (81,84,85)

-- lista de pedidos
UPDATE	purchase_order_line	
	SET  product_uom = 127
	WHERE product_uom IN (81,84,85)

-- movimiento de stock
UPDATE stock_move
	SET  product_uom = 127
	WHERE product_uom IN (81,84,85)

DELETE FROM product_uom WHERE ID IN (81,84,85)


