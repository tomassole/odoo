-- LISTADO DE SQL PARA ARREGLAR LAS UNIDADES CUANDO ESTAN DUPLICADAS O MAL DEFINIDADS REEMPLAZANDO LA CORRECTA.


select product_id, bom_id  from mrp_bom_line
	group by product_id, bom_id
	order by product_id;

	select * from mrp_bom_line

UPDATE mrp_bom_line 
	SET  bom_id = 69
	WHERE bom_id IN (67, 70,253,254,255,256,246,247,248,259) ;


UPDATE mrp_bom_line 
	SET  bom_id = 208
	WHERE bom_id IN (293,207,310,355,311,312,203,204,205,206);

UPDATE mrp_bom_line 
	SET  bom_id = 115
	WHERE bom_id IN (178,216,121,179,180,188,189)


UPDATE mrp_bom_line 
	SET  bom_id = 127
	WHERE bom_id IN (81,84,85)

UPDATE mrp_bom_line 
	SET  bom_id = 146
	WHERE bom_id IN (37,96,105,120,122,224,228,229,231,232,233,243,268,270,271,273,288,299)
update product_template 
	set uom_id = 146 
	WHERE uom_id IN (37,96,105,120,122,224,228,229,231,232,233,243,268,270,271,273,288,299);	
update product_template 
	set uom_po_id = 146
	WHERE uom_po_id IN (37,96,105,120,122,224,228,229,231,232,233,243,268,270,271,273,288,299);	
UPDATE	purchase_order_line	
	SET  product_uom = 146
	WHERE product_uom IN (37,96,105,120,122,224,228,229,231,232,233,243,268,270,271,273,288,299);	
UPDATE stock_move
	SET  product_uom = 146
	WHERE product_uom IN (37,96,105,120,122,224,228,229,231,232,233,243,268,270,271,273,288,299);	

	
